+
+  /// 0..1  Vignette-Randabdunklung
+  final double vignette;
+
+  /// Farbsttigung (1 = normal, <1 = gebleached)
+  final double saturation;
+
+  /// Wei-Wash (0..1) als leichte Aufhellung
+  final double wash;
+
