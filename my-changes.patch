+
+    final moodLabel = (r.moodLabel ?? 'Neutral').trim();
+
+    final v = {
+      'id': entryId,
+      'ts': ts.toIso8601String(),
+      'type': 'reflection',
+      'text': textForCard,
+      'input': {'mode': r.mode, 'content': r.userInput},
