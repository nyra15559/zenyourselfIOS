+      if (n < 1000000000000) {
+        // Sekunden
+        return DateTime.fromMillisecondsSinceEpoch(v.toInt() * 1000, isUtc: true);
+      } else if (n < 10000000000000000) {
+        // Millisekunden
+        return DateTime.fromMillisecondsSinceEpoch(v.toInt(), isUtc: true);
